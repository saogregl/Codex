datasource db {
    provider = "sqlite"
    url      = "file:dev.db"
}
 
generator client {
    provider    = "cargo prisma"
    output      = "../../crates/prisma/src/prisma"
    module_path = "codex_prisma::prisma"
    client_format = "folder"
}
 
model User {
    id          String  @id
    displayName String
}
